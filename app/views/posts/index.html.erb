<div class="navbar">
  <% if user_signed_in? %>
    Welcome! <%= current_user.username%>
  <% end %>
</div>
<%= link_to 'New post', new_post_path%>
<h1>Timeline</h1>
<% @posts.each do |post| %>
  <%= post.title %>
  <%= post.body %>
  <%= post.user.username %>
  <%= link_to 'Add comment', new_post_comment_path(post_id: post.id)%>
<% end %>